<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.parktaeim.settingsui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Activity.SettingsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.SettingsBluetoothActivity"/>
        <activity android:name=".Activity.SettingsLocationActivity"/>
        <activity android:name=".Activity.SettingsLocationActivity2"/>
        <activity android:name=".Activity.SettingsWifiActivity"/>
        <activity android:name=".Activity.DeviceWallpaperActivity"/>
        <activity android:name=".Activity.DeviceCapacityActivity"/>
        <activity android:name=".Activity.DeviceInfoActivity"/>
        <activity android:name=".Activity.DeviceLanguageActivity"/>
        <activity android:name=".Activity.DeviceResetActivity"/>
        <activity android:name=".Activity.DeviceSoftwareUpdateActivity"/>

    </application>

</manifest>